                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#hasTopping"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">3</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#PizzaTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
