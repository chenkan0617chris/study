        <rdfs:range rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#PizzaTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#isBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#isBaseOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#isIngredientOf"/>
    


    <!-- http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#isToppingOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#isToppingOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#isIngredientOf"/>
    </owl:ObjectProperty>
    


