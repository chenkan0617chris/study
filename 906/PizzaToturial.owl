        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#JalapenoPepperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
