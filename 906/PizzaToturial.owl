                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that only has Mozzarella, Tomato, and Pepperoni toppings</rdfs:comment>
    </owl:Class>
    


