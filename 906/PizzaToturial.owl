    <!-- http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#hasBase">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#isBaseOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#PizzaBase"/>
        <owl:propertyDisjointWith rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#hasTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#hasIngredient">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#hasSpiciness">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#PizzaTopping"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#Spiciness"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#hasTopping">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#isToppingOf"/>
