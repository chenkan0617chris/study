    <!-- http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#hasCaloricContent -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#hasCaloricContent">
