                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/chenkan/ontologies/2023/7/PizzaToturial#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
